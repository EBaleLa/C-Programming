#include<iostream>
#include <algorithm> 
#include<vector>
using namespace std;
int main(){
	char* p;
	string s = "Hello World";
	char c[] = {'h','e','l','l','o' ,' ', 'w','o','r','l','d'};
	bool found = false;
	char key;
	cout << (int)c[0] << endl;
	cout << (char)104 << endl;
	cout << (char)(104-32) << endl;
	vector<char> v(c,c+11);
	sort(v.begin(),v.end());
	key = 'l';
	found = binary_search(v.begin(),v.end(),key);
	if(found)
		cout << "Found " << key;  
		else
			cout << "Not Fount " << key;
